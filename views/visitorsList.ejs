<!DOCTYPE html>
<html>
<head>
    <title>Visitors List</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css">
    <style>
        .expired {
            color: red;
        }
    </style>
</head>
<body>
<div class="container">
    <h1>Visitors List</h1>

    <table class="table table-striped">
        <thead>
        <tr>
            <th>Name</th>
            <th>Phone Number</th>
            <th>Vehicle Number</th>
            <th>Entry Time</th>
            <th>Expired</th>
        </tr>
        </thead>
        <tbody>
        <% for(let visitor of visitors) { %>
            <tr <% if (visitor.isExpired) { %> class="expired" <% } %>>
                <td><%= visitor.name %></td>
                <td><%= visitor.phone_number %></td>
                <td><%= visitor.vehicle_number %></td>
                <td><%= visitor.entry %></td>
                <td><%= visitor.isExpired ? 'Yes' : 'No' %></td>
            </tr>
        <% } %>
        </tbody>
    </table>
</div>
</body>
</html>